# Ruflo Environment Configuration - Kimi-First Fork
# Copy this file to .env and fill in your API keys

# REQUIRED: Moonshot API Key for Kimi (Default Provider)
MOONSHOT_API_KEY=your_moonshot_api_key_here

# Optional: Anthropic API Key for Claude (Fallback)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: OpenAI API Key for GPT models  
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Google API Key for Gemini models
GOOGLE_API_KEY=your_google_api_key_here

# Ruflo Configuration
RUFLO_LOG_LEVEL=info
RUFLO_MAX_AGENTS=10
RUFLO_PORT=8080

# Default Provider Settings
DEFAULT_PROVIDER=moonshot
DEFAULT_MODEL=kimi-k2-5

# Optional: Disable telemetry
RUFLO_DISABLE_TELEMETRY=false
