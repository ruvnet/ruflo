  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@claude-flow/ruvector-upstream": ">=3.0.0-alpha.1"
  },
  "peerDependenciesMeta": {
    "@claude-flow/ruvector-upstream": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  },
