{
  "name": "@claude-flow/plugin-test-intelligence",
  "version": "3.0.0-alpha.1",
  "description": "AI-powered test intelligence plugin for predictive test selection, flaky detection, and coverage optimization",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tools": {
      "import": "./dist/mcp-tools.js",
      "types": "./dist/mcp-tools.d.ts"
    },
    "./bridges": {
      "import": "./dist/bridges/index.js",
      "types": "./dist/bridges/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
